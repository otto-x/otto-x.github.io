<!DOCTYPE html>
<html>

<head>
  <meta charset="utf-8">

  <% var title=page.title; %>

    <title>
      <% if (title){ %>
        <%= title %> | <% } %>
            <%= config.title %>
    </title>
    <meta name="viewport" content="width=device-width,initial-scale=1,maximum-scale=1">
    <meta name='robots' content='noindex,nofollow' />

    <!-- <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/KaTeX/0.5.1/katex.min.css"> -->

    <!-- <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/katex@0.12.0/dist/katex.css"
      integrity="sha384-qCEsSYDSH0x5I45nNW4oXemORUZnYFtPy/FqB/OjqxabTMW5HVaaH9USK4fN3goV" crossorigin="anonymous"> -->

    <script src="/js/Valine.min.js"></script>

    <% if (config.lunr) { %>
      <script src="<%= url_for('js/compatibility/augment.js') %>" type="text/javascript"></script>
      <% } %>
        <% if (config.feed) { %>
          <% if (config.feed.type=='atom' ) { %>
            <link rel="alternate" href="<%= url_for(config.feed.path) %>" title="<%= config.title %>"
              type="application/atom+xml" />
            <% }else if (config.feed.type=='rss2' ) { %>
              <link rel="alternate" href="<%= url_for(config.feed.path) %>" title="<%= config.title %>"
                type="application/rss+xml" />
              <% } %>
                <% } %>

                  <script src="https://cdn.bootcss.com/jquery/2.2.0/jquery.min.js"></script>
                  <link rel="stylesheet" href="<%= url_for('css/normalize.css') %>" type="text/css">
                  <link rel="stylesheet" href="<%= url_for('css/search.css') %>" type="text/css">

                  <link rel="stylesheet" href="<%= url_for('css/basscss.css') %>" type="text/css">
                  <!-- <link href="https://cdn.bootcss.com/basscss/8.1.0/css/basscss.css" rel="stylesheet" type="text/css"> -->
                  <link rel="stylesheet" href="<%= url_for('css/basscss-ui-groups.css') %>" type="text/css">
                  <link rel="stylesheet" href="<%= url_for('css/style.css') %>" type="text/css">
                  <link rel="stylesheet" href="<%= url_for('css/prettify.css') %>" type="text/css">
                  <!-- <link href="https://rachel.objectstore.co/dist/fonts/lato-v13/lato-v13-all.css" rel="stylesheet" type="text/css"> -->
                  <!--[if lt IE 9]><script src="<%- config.root %>js/html5.js" type="text/javascript"></script><![endif]-->
                  <script src="<%= url_for('js/main.js') %>" type="text/javascript"></script>
                  <link href="https://use.sevencdn.com/css?family=Lora|Noto+Serif+SC&display=swap" rel="stylesheet">
                  <style>
                    h1,
                    h2,
                    h3,
                    h4,
                    h5,
                    h6,
                    body {
                      font-family: 'Lora', 'Noto Serif SC', serif;
                    }

                    pre {
                      font-family: 'Fira Code', monospace;
                    }
                  </style>
</head>